#include<stdio.h>
# define NMAX 50
int main()
/*有n个人围成一圈,顺序编号.
从第一个人开始报数(从1到3),凡是报道3的人退出圈子
问最后留下来的是原来第几号的那位*/
{
	int i,t, n, a[NMAX];//i是循环人数的初始编号;t是减少后的总人数;n是总人数;
	int m = 0;//m是123编号
	scanf("%d", &n);
	t = n;
	for (i = 1; i <= n; i++)//将人数初始编号存入数组
	{
		a[i] = i;
	}
	for (i = 1; i <= n; i++)//循环进行123编号
	{
		if (a[i] != 0)//如果!=0就进行编号
		{
			m++;
		}
		if (3 == m)//当编号为3的人定义为0(即退出队伍);并且重新从0开始编号
		{
			a[i] = 0;
			t--;
			m = 0;
		}
		if (n == i)//如果等于最大人数就进行队伍循环
		{
			i = 0;
		}
		if (1 == t)//最后剩余的一个人退出编号
		{
			break;
		}
	}
	for (i = 1; i <= n; i++)//打印最后一名的编号
	{
		if (a[i] != 0)
			printf("%d ", a[i]);
	}
	return 0;
}
